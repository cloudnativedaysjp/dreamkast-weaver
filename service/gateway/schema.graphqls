# GraphQL schema example

type VotingPaper {
  conferenceName: String!
  talkId: Int!
}

input VoteCfpInput {
  conferenceName: String!
  talkId: Int!
}

type VoteCfpPayload {
  votingPaper: VotingPaper!
}


type Query {
  votingPapers: [VotingPaper!]!
}

type Mutation {
  voteCfp(input: VoteCfpInput!): VoteCfpPayload!
}
